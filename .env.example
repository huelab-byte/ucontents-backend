APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost
# Frontend URL for OAuth callback. Use the same protocol (http/https) and port as your frontend.
# Local callback URLs (add these to provider apps): http://localhost:3000/app/facebook/page, /app/facebook/profile, /app/instagram/profile, /app/youtube/channel, /app/tiktok/profile
# If using https locally, set FRONTEND_URL=https://localhost:3000
FRONTEND_URL=http://localhost:3000
# OAuth uses frontend for callback by default. Set false to use backend URL (APP_URL/api/.../callback).
OAUTH_REDIRECT_USE_FRONTEND=true
# TikTok live app: set exact callback URL if required (e.g. https://ucontents.com/app/tiktok/profile)
# TIKTOK_OAUTH_CALLBACK_URL=

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
# Use "warning" or "error" for faster local dev (less I/O); "debug" for troubleshooting
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=backend
DB_USERNAME=root
DB_PASSWORD=

# For faster local dev with php artisan serve, use SESSION_DRIVER=file (and CACHE_STORE=file)
SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
# Queue: redis in both development and production (run: php artisan queue:work redis)
QUEUE_CONNECTION=redis

# For faster local dev with php artisan serve, use CACHE_STORE=file
CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# AI Integration Configuration
# Provider options: openai, azure_openai, anthropic, google, deepseek, xai, ucontents
MEDIA_UPLOAD_AI_PROVIDER=openai
MEDIA_UPLOAD_VISION_MODEL=gpt-4o
MEDIA_UPLOAD_TEXT_MODEL=gpt-4o
FOOTAGE_METADATA_AI_PROVIDER=openai
FOOTAGE_VISION_MODEL=gpt-4o

# Self-hosted AI (Ucontents Provider)
# Set MEDIA_UPLOAD_AI_PROVIDER=ucontents or FOOTAGE_METADATA_AI_PROVIDER=ucontents to use
EXTERNAL_AI_CHAT_URL=https://gpt.ucontents.com
EXTERNAL_AI_CHAT_API_KEY=pk_prod_tbQFGQFKIb8SeyvaPqAJX7nrXk7ZRlJU
EXTERNAL_AI_CHAT_TIMEOUT=120

VITE_APP_NAME="${APP_NAME}"
